env:
  es6: true
  node: true
extends: eslint:recommended
rules:
  indent: [error, 4, {SwitchCase: 1}]
  linebreak-style: [error, unix]
  quotes: [error, single]
  semi: [error, always]
  no-console: warn
  no-template-curly-in-string: error
  consistent-return: on
  brace-style: [error, allman, {allowSingleLine: true}]
  no-unused-vars: warn
